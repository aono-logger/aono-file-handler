{
  "name": "aono-file-handler",
  "version": "0.2.1",
  "description": "file handler with formatting and rotation support for aono logger",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf lib #",
    "compile-ts": "tsc",
    "compile-coffee": "coffee -m -c -o lib src",
    "compile": "npm run compile-ts && npm run compile-coffee",
    "test": "mocha --use_strict lib/mocha.config.js $(find lib -name \"*.spec.js\")",
    "copy-package.json": "mkdir -p lib/ && cp package.json lib/",
    "build": "npm run compile && npm run copy-package.json && npm run test",
    "clean-build": "npm run clean && npm run build",
    "watch": "watch 'npm run clean-build' src",
    "deploy": "npm run clean-build && npm publish lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aono-logger/aono-file-handler.git"
  },
  "keywords": [
    "file",
    "handler",
    "aono",
    "node",
    "logger",
    "log",
    "logging",
    "typescript"
  ],
  "author": "Maciej Cha≈Çapuk <maciej@chalapuk.pl>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/aono-logger/aono-file-handler/issues"
  },
  "homepage": "https://github.com/aono-logger/aono-file-handler#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^9.6.18",
    "aono": "^0.4.3",
    "coffeescript": "^2.3.0",
    "fake-promise": "^1.3.0",
    "glob": "^7.1.2",
    "mocha": "^5.2.0",
    "should": "^13.2.1",
    "should-sinon": "0.0.6",
    "sinon": "^4.5.0",
    "source-map-support": "^0.5.6",
    "typescript": "^2.8.3"
  }
}
